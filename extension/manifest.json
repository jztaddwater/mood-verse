{
  	"manifest_version": 2,
    "name": "The New Tab Mood-Based Bible Verse extension",
    "description": "New tab",
    "version": "0.0.0.1",
    "permissions" : ["management"],
    "content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["js/jquery-2.1.4.min.js"]
		}
	],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
 }